#include <stdio.h>

int main() {
    int arr[] = {3, -2, 5, -7, 0, 9, -4};
    int n = sizeof(arr) / sizeof(arr[0]);
    int i;

    printf("Positive numbers: ");
    for (i = 0; i < n; i++) {
        if (arr[i] > 0) {
            printf("%d ", arr[i]);
        }
    }

    printf("\nNegative numbers: ");
    for (i = 0; i < n; i++) {
        if (arr[i] < 0) {
            printf("%d ", arr[i]);
        }
    }

    return 0;
}
